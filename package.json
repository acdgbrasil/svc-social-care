{
  "name": "@acdg/svc-social-care",
  "version": "0.1.0",
  "private": true,
  "description": "ACDG svc-social-care service baseline.",
  "type": "module",
  "engines": {
    "bun": ">=1.1.0"
  },
  "scripts": {
